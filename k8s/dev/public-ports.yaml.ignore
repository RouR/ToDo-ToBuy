#metrics http://192.168.99.100:18080/nginx_status
kind: Service
apiVersion: v1
metadata:
  namespace: ingress-nginx
  name: nginx-ingress-prometheus-discovery
  labels:
    k8s-app: ingress
spec:
  selector:
    app: ingress-nginx
  externalIPs:
  - 192.168.99.100  
  type: NodePort
  ports:
  - name: metrics
    port: 18080
    protocol: TCP       
---
